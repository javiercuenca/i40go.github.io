    <!-- http://www.purl.org/i4go/domain-task/soa/EDIspecification -->

    <owl:Class rdf:about="http://www.purl.org/i4go/domain-task/soa/EDIspecification">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/i4go/domain-task/soa/ServiceDescriptionLanguage"/>
        <rdfs:comment xml:lang="en">Electronic data interchange (EDI) is the concept of businesses electronically communicating information that was traditionally communicated on paper, such as purchase orders and invoices. Technical standards for EDI exist to facilitate parties transacting such instruments without having to make special arrangements.</rdfs:comment>
        <rdfs:label xml:lang="en">EDI specification</rdfs:label>
    </owl:Class>
    


