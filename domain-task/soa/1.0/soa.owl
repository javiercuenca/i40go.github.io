        <rdfs:label xml:lang="en">Service description language</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/i4go/domain-task/soa/ServiceOrientedArchitectureFunctionPattern -->

    <owl:Class rdf:about="http://www.purl.org/i4go/domain-task/soa/ServiceOrientedArchitectureFunctionPattern">
