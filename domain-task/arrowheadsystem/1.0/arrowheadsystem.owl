    <!-- http://www.purl.org/i4go/domain-task/arrowheadsystem/AHapplicationSystem -->

    <owl:Class rdf:about="http://www.purl.org/i4go/domain-task/arrowheadsystem/AHapplicationSystem">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/i4go/domain-task/arrowheadsystem/AHsystem"/>
