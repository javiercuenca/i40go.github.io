        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Operation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-SO#Workshop"/>
        <rdfs:comment xml:lang="en">This class is used to relate operations with their related working structures.</rdfs:comment>
        <rdfs:label xml:lang="en">has workshop</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-SO#isEquipmentOf -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-SO#isEquipmentOf">
