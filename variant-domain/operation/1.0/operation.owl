        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Operation"/>
        <rdfs:range rdf:resource="http://www.purl.org/i4go/variant-domain/operation/TimeConstraints"/>
        <rdfs:comment xml:lang="en">This class is used to relate operations with their time constraints.</rdfs:comment>
        <rdfs:label xml:lang="en">has time constraints</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-SO#hasWorkshop -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-SO#hasWorkshop">
