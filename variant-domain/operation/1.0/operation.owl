        <rdfs:domain rdf:resource="http://www.purl.org/i4go/variant-domain/operation/SimulationPart"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Operation"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Technology"/>
        <rdfs:range rdf:resource="http://www.purl.org/i4go/variant-domain/operation/SimulationRoute"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-SO#Route"/>
        <rdfs:comment xml:lang="en">This property is used to relate operations with their routes and simulation routes.</rdfs:comment>
        <rdfs:label xml:lang="en">has route</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-SO#hasTechnology -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-SO#hasTechnology">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Operation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-SO#Technology"/>
        <rdfs:label xml:lang="en">This property is used to relate operations with their associated equipment and technologies.</rdfs:label>
        <rdfs:label xml:lang="en">has technology</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-SO#hasTimeConstraints -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-SO#hasTimeConstraints">
