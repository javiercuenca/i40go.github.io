  <rdf:Description rdf:about="http://www.purl.org/i40go/variant-domain/manufacturingresource/HumanResource">
    <rdfs:label xml:lang="en">Human resource</rdfs:label>
    <rdfs:comment xml:lang="en">The personnel of a business or organization, regarded as a significant asset in terms of skills and abilities.</rdfs:comment>
