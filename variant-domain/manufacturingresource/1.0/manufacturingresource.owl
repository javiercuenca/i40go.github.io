    <owl:Class rdf:about="http://www.purl.org/i4go/variant-domain/manufacturingresource/GeographicResource">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/mason.owl#Resource"/>
        <rdfs:comment xml:lang="en">A gepgraphic resource is any physical material constituting part of Earth that people need and value.</rdfs:comment>
        <rdfs:label xml:lang="en">Geographic resource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/i4go/variant-domain/manufacturingresource/HumanResource -->

    <owl:Class rdf:about="http://www.purl.org/i4go/variant-domain/manufacturingresource/HumanResource">
