    </owl:Class>
    


    <!-- http://www.purl.org/i4go/variant-domain/manufacturingresource/BuildingZone -->

    <owl:Class rdf:about="http://www.purl.org/i4go/variant-domain/manufacturingresource/BuildingZone">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/i4go/variant-domain/manufacturingresource/GeographicResource"/>
        <rdfs:comment xml:lang="en">Each zone determines a building&apos;s shape and permitted uses. A building&apos;s shape is controlled by zonal restrictions on allowable floor area ratio and height (in absolute terms and in relation with adjacent buildings and roads).</rdfs:comment>
        <rdfs:label xml:lang="en">Building zone</rdfs:label>
    </owl:Class>
    


