<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://www.purl.org/i4go/variant-domain/productionplant#"
    xmlns:ns="http://creativecommons.org/ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" > 
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#hasPlantName">
    <rdfs:label xml:lang="en">has plant name</rdfs:label>
    <rdfs:comment xml:lang="en">The name of the plant</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#hasPlantInternal">
    <rdfs:label xml:lang="en">has plant internal</rdfs:label>
    <rdfs:comment xml:lang="en">Does the plant belong to the company or to an external partner</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#Plant">
    <rdfs:label xml:lang="en">Plant</rdfs:label>
    <rdfs:comment xml:lang="en">The “Plant” entity describes the plant as a physical location of one or more fabs. The Plant belongs to a supply chain and can either be internal or external. (master entity)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/mason.owl#Resource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/issued">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.purl.org/i4go/variant-domain/productionplant">
    <terms:issued xml:lang="en">2021-05-25</terms:issued>
    <rdfs:comment xml:lang="en">The 'Production Plant' ontology module represents data related to plant topology, including the concrete equipment of an actual project – the instance data.</rdfs:comment>
    <dc:title xml:lang="en">Production Plant ontology</dc:title>
    <terms:creator xml:lang="en">Javier Cuenca &lt;mailto:jcuenca@mondragon.edu&gt;
Felix Larrinaga &lt;mailto:flarrinaga@mondragon.edu&gt;</terms:creator>
    <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    <owl:imports rdf:resource="http://www.purl.org/i4go/variant-domain/manufacturingresource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#hasFab">
    <rdfs:label xml:lang="en">has fab</rdfs:label>
    <rdfs:comment xml:lang="en">This property is used to relate a production plant with the factory ot belongs to</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-SO#Fab"/>
    <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-SO#Plant"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#hasPlantID">
    <rdfs:label xml:lang="en">has plant ID</rdfs:label>
    <rdfs:comment xml:lang="en">Will generally refer to the geographical location of the plant</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://creativecommons.org/ns#license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3id.org/ecsel-dr-SO#Fab">
    <rdfs:label xml:lang="en">Fab</rdfs:label>
    <rdfs:comment xml:lang="en">The “Fab” entity depicts the fab as a technological and physical production unit. (master entity)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.purl.org/i4go/variant-domain/manufacturingresource/GeographicResource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
</rdf:RDF>
