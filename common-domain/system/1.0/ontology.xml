  <rdf:Description rdf:about="http://www.w3.org/ns/ssn/hasSubSystem">
    <rdfs:label xml:lang="en">has subsystem</rdfs:label>
    <rdfs:comment xml:lang="en">Relation between a System and its component parts.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/ssn/System">
    <rdfs:label xml:lang="en">System</rdfs:label>
    <rdfs:comment xml:lang="en">A system is considered as a set of interrelated elements (i.e., technical systems, network systems) considered in a defined context as a whole and separated from their environment. A system is generally defined with the view of achieving a given objective, for example by performing a definite function.</rdfs:comment>
    <rdfs:subClassOf rdf:nodeID="A0"/>
    <rdfs:subClassOf rdf:nodeID="A2"/>
    <rdfs:subClassOf rdf:nodeID="A3"/>
    <rdfs:subClassOf rdf:nodeID="A4"/>
    <rdfs:subClassOf rdf:nodeID="A5"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.purl.org/i40go/common-domain/system/SystemDocumentation">
    <rdfs:label xml:lang="en">System documentation</rdfs:label>
    <rdfs:comment xml:lang="en">The collection of documents that describes the requirements, capabilities, limitations, design, operation, and maintenance of a system, such as a communications, computing, or information processing system</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A5">
    <owl:allValuesFrom rdf:resource="http://www.w3id.org/ecsel-dr-PWR#Deployment"/>
    <owl:onProperty rdf:resource="http://www.purl.org/i40go/common-domain/system/hasDeployment"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/ssn/deployedSystem">
    <rdfs:label xml:lang="en">deployed system</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PWR#Deployment"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A6">
    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
    <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#orchestratedBy"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/i40/aml#hasName">
    <rdfs:label>has name</rdfs:label>
    <rdfs:comment xml:lang="en">This property is used to specify the name of an entity, such a system.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
