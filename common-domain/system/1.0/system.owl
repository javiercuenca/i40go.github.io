    <owl:Class rdf:about="http://www.w3.org/ns/ssn/System">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.purl.org/i4go/common-domain/system/hasDeployment"/>
