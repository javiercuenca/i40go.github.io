    <!-- http://www.purl.org/i4go/common-domain/system/Component -->

    <owl:Class rdf:about="http://www.purl.org/i4go/common-domain/system/Component">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">According to ISO26262: non-system level element (3.41) that is logically or technically separable and is comprised of more than one hardware part (3.71) or one or more software units (3.159). 

A component is a part of a system (3.163).</rdfs:comment>
        <rdfs:label xml:lang="en">Component</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/i4go/common-domain/system/SystemDocumentation -->

    <owl:Class rdf:about="http://www.purl.org/i4go/common-domain/system/SystemDocumentation">
        <rdfs:comment xml:lang="en">The collection of documents that describes the requirements, capabilities, limitations, design, operation, and maintenance of a system, such as a communications, computing, or information processing system</rdfs:comment>
        <rdfs:label xml:lang="en">System documentation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#Composition -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#Composition">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#orchestratedBy"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#orchestratedBy"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Composition</rdfs:label>
        <rdfs:label xml:lang="en">This class is used to represent the components of a system</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#Process -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#Process">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2010/01/core-soa.owl#Composition"/>
        <rdfs:comment xml:lang="en">Processes are all the related activities (parts) inside the system that work together to make it function.</rdfs:comment>
        <rdfs:label xml:lang="en">Process</rdfs:label>
    </owl:Class>
    


